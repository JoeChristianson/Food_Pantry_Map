<section>

<h2>Login</h2>

<form class="form login-form">
  <div class="form-group">
    <label for="email-login">email:</label>
    <input class="form-input" type="text" id="email-login" />
  </div>
  <div class="form-group">
    <label for="password-login">password:</label>
    <input class="form-input" type="password" id="password-login" />
  </div>
  <div class="form-group">
    <button type="submit">login</button>
  </div>
</form>
</section>
<section>

<h2>Register</h2>

<form class="form register-form">
      <div class="form-group">
    <label for="username-register">User Name:</label>
    <input class="form-input" type="text" id="username-register" />
  </div>
  <div class="form-group">
    <label for="email-register">Enter email:</label>
    <input class="form-input" type="text" id="email-register" />
  </div>
  <div class="form-group">
    <label for="password-register">Choose password:</label>
    <input class="form-input" type="password" id="password-register" />
  </div>
    <div class="form-group">
    <label for="password-confirm">Confirm password:</label>
    <input class="form-input" type="password" id="password-confirm" />
  </div>
  <div class="form-group">
    <label for="pantry-name-register">Pantry Name:</label>
    <input class="form-input" type="text" id="pantry-name-register" />
  </div>
    <div class="form-group">
    <label for="pantry-address-register">Street Address:</label>
    <input class="form-input" type="text" id="pantry-address-register" />
  </div>
    </div>
    <div class="form-group">
    <label for="pantry-city-register">City and State:</label>
    <input class="form-input" type="text" id="pantry-city-register" />
  </div>
      <div class="form-group">
    <label for="pantry-latitude-register">Latitude:</label>
    <input class="form-input" type="text" id="pantry-latitude-register" />
  </div>
        <div class="form-group">
    <label for="pantry-longitude-register">Longitude:</label>
    <input class="form-input" type="text" id="pantry-longitude-register" />
  </div>
          <div class="form-group">
    <label for="pantry-phone-register">Contact Phone:</label>
    <input class="form-input" type="text" id="pantry-phone-register" />
  </div>
  <div class="form-group">
    <button type="submit">register</button>
  </div>
</form>
</section>

<script src="./js/login.js"></script>